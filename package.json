{
  "name": "lastwhisper-discord-bot",
  "version": "4.8.0",
  "description": "The Discord Bot for the Free Company LastWhisper of the game Final Fantasy XIV.",
  "bin": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "node build/index.js",
    "watch": "nodemon src/index.ts",
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "test": "jest",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "eslint --fix .",
    "typeorm": "typeorm-ts-node-esm -d src/config/migrationDataSource.ts",
    "orm:generate": "yarn typeorm migration:generate",
    "orm:create": "yarn typeorm migration:create",
    "orm:migrate": "yarn typeorm migration:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "author": "Shadowking124",
  "license": "GPL-3.0-only",
  "type": "module",
  "private": true,
  "dependencies": {
    "commander": "^10.0.0",
    "discord-api-types": "^0.37.32",
    "discord.js": "^14.7.1",
    "inquirer": "^9.1.4",
    "luxon": "^3.2.1",
    "pg": "^8.9.0",
    "pino": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.2.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
