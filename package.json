    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^8.2.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@types/sinon": "^10.0.11",
    "@types/tap": "^15.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
