    "build": "tsc -b",
    "test": "tap --node-arg=--loader=ts-node/esm --no-coverage",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShadowKing345/LastWhisper-Discord-Bot.git"
  },
